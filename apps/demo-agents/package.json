{
  "name": "@nexus-402/demo-agents",
  "version": "1.0.0",
  "description": "Demo AI agents for NEXUS-402 - Showcasing the protocol capabilities",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "start:defi": "tsx src/agents/defi-strategist.ts",
    "start:payment": "tsx src/agents/payment-processor.ts",
    "start:oracle": "tsx src/agents/price-oracle.ts",
    "start:all": "tsx src/index.ts"
  },
  "dependencies": {
    "@nexus-402/sdk": "workspace:*",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "nexus-402",
    "x402",
    "ai-agents",
    "demo",
    "cronos"
