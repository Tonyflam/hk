{
  "name": "@nexus-402/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for integrating with NEXUS-402 Protocol on Cronos",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "nexus-402",
    "x402",
    "cronos",
    "crypto.com",
    "ai-agents",
    "defi",
    "payments",
    "blockchain",
    "web3"
  ],
  "author": "NEXUS-402 Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexus-402/nexus-402"
  },
  "homepage": "https://nexus-402.dev"
}
